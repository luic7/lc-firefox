{
  "manifest_version": 2,
  "name": "lc-shortcuts",
  "version": "1.8",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs", "browserSettings"],
  "commands": {
    "next-tab": {
      "suggested_key": {
        "default": "Ctrl+9"
      },
      "description": "Go to next tab"
    },
    "prev-tab": {
      "suggested_key": {
        "default": "Ctrl+8"
      },
      "description": "Go to previous tab"
    },
    "prior-tab": {
      "suggested_key": {
        "default": "Ctrl+0"
      },
      "description": "Go to previous tab"
    },
    "stt-1": {
      "suggested_key": {
        "default": "Ctrl+1"
      },
      "description": "Switch to tab 1"
    },
    "stt-2": {
      "suggested_key": {
        "default": "Ctrl+2"
      },
      "description": "Switch to tab 2"
    },
    "stt-3": {
      "suggested_key": {
        "default": "Ctrl+3"
      },
      "description": "Switch to tab 3"
    },
    "stt-4": {
      "suggested_key": {
        "default": "Ctrl+4"
      },
      "description": "Switch to tab 4"
    },
    "stt-5": {
      "suggested_key": {
        "default": "Ctrl+5"
      },
      "description": "Switch to tab 5"
    },
    "stt-6": {
      "suggested_key": {
        "default": "Ctrl+6"
      },
      "description": "Switch to tab 6"
    },
    "stt-7": {
      "suggested_key": {
        "default": "Ctrl+7"
      },
      "description": "Switch to tab 7"
    },
    "close-others": {
      "suggested_key": {
        "default": "F8"
      },
      "description": "Close other tabs"
    },
    "close-tab": {
      "suggested_key": {
        "default": "F7"
      },
      "description": "Close tab and go to left"
    }
  }
}
